. "$HOME/.cargo/env"

# Local binaries
export PATH=$PATH:~/.local/bin:~/bin
# Android sdk
export PATH=$PATH:/opt/android/sdk/cmdline-tools/latest/bin:/opt/android/sdk/platform-tools:/opt/android/sdk/build-tools/36.0.0:/opt/android/sdk/emulator
export ANDROID_HOME=/opt/android/sdk
# Android tools
export PATH=$PATH:/opt/android/magisk-v30.6:/opt/jadx/bin:/opt/gradle-8.10.1/bin:/opt/smali

